{
  "name": "react-component",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "jest --watch && echo \"test succeeded.\"",
    "test:update": "jest --updateSnapshot --watch && echo \"Snapshot has successfully updated.\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "fix": "yarn lint:fix & yarn format",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build-type-guards": "type-predicates-generator -f 'src/models/*.ts' -o 'src/utils/commons/typeGuardsUtils.ts' && prettier --write 'src/utils/commons/typeGuardsUtils.ts'",
    "make-language-json": "npx ts-node src/i18n/makeJsonFiles.ts"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "printWidth": 100
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@headlessui/tailwindcss": "^0.1.1",
    "@hookform/resolvers": "^2.9.10",
    "@radix-ui/react-toast": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.0.2",
    "@tanstack/react-table": "^8.7.3",
    "@types/js-cookie": "^3.0.2",
    "date-fns": "^2.29.3",
    "decimal.js": "^10.4.3",
    "i18next": "^22.0.6",
    "i18next-browser-languagedetector": "^7.0.1",
    "js-cookie": "^3.0.1",
    "react": "^18.2.0",
    "react-datepicker": "^4.8.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.40.0",
    "react-i18next": "^12.0.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.4.4",
    "recoil": "^0.7.6",
    "swr": "^2.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@jest/globals": "^29.3.1",
    "@storybook/addon-actions": "^7.0.5",
    "@storybook/addon-essentials": "^7.0.5",
    "@storybook/addon-interactions": "^7.0.5",
    "@storybook/addon-links": "^7.0.5",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^7.0.5",
    "@storybook/react-vite": "^7.0.5",
    "@storybook/testing-library": "^0.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.0.8",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "clsx": "^1.2.1",
    "eslint": "^8.28.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-tailwindcss": "^3.7.1",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-css-modules": "^2.1.0",
    "jest-environment-jsdom": "^29.3.1",
    "lint-staged": "^13.0.4",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "react-paginate": "^8.1.4",
    "react-test-renderer": "^18.2.0",
    "storybook": "^7.0.5",
    "tailwindcss": "^3.3.1",
    "ts-jest": "^29.0.3",
    "ts-jest-mock-import-meta": "^1.0.0",
    "type-predicates-generator": "^0.3.2",
    "typescript": "^4.9.3",
    "vite": "^3.2.3",
    "whatwg-fetch": "^3.6.2"
  }
